proj[customername]
	(sel[branch_city=Corinth]
		(join[branch.branch_name+5=branch_name+2] (branch)(
			join[account_number=customer.account_number]((union(chekingaccounts)(savingsaccounts)))(customer))
		)
	)



union(savingaccounts)(proj[account_number,branch_name,branch_city,customer_id](chekingaccounts))
